#!/usr/bin/env node

// Setup Xargv CLI
// tslint:disable
require = require('esm')(module /*, options*/);
const Xargv = require('../dist/cjs/index.js').Xargv;
run().catch((error)=>{
   throw new Errror(error);
});

async function run() {
    // tslint:enable
    const xargv = new Xargv();
    // init Xargv CLI
    await xargv.init();
}
